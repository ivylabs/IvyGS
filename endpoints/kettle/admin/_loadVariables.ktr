        <class_source><![CDATA[private FieldHelper NameOut = null;
private FieldHelper NameIn = null;
private FieldHelper MainFileOut = null;
private FieldHelper MainFileIn = null;
private FieldHelper GitCloneURLOut = null;
private FieldHelper GitCloneURLIn = null;
private FieldHelper GitUsernameOut = null;
private FieldHelper GitUsernameIn = null;
private FieldHelper GitPasswordOut = null;
private FieldHelper GitPasswordIn = null;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
		NameOut = get(Fields.Out,"ProjectName");
		NameIn = get(Fields.In,"ProjectName");
		MainFileOut = get(Fields.Out,"MainFile");
		GitCloneURLOut = get(Fields.Out,"GitCloneURL");
		GitCloneURLIn = get(Fields.In,"GitCloneURL");
		GitUsernameOut = get(Fields.Out,"GitUsername");
		GitUsernameIn = get(Fields.In,"GitUsername");
		GitPasswordOut = get(Fields.Out,"GitPassword");
		GitPasswordIn = get(Fields.In,"GitPassword");
    }
	
	String inputName = NameIn.getString(r);
	String inputMainFile = "main";
	String inputGitCloneURL = GitCloneURLIn.getString(r);
	String inputGitUsername = GitUsernameIn.getString(r);
	String inputGitPassword = GitPasswordIn.getString(r);


    r = createOutputRow(r, data.outputRowMeta.size());
	
    NameOut.setValue(r,parent.environmentSubstitute(inputName));
	MainFileOut.setValue(r,parent.environmentSubstitute(inputMainFile));
	GitCloneURLOut.setValue(r,parent.environmentSubstitute(inputGitCloneURL));
	GitUsernameOut.setValue(r,parent.environmentSubstitute(inputGitUsername));
	GitPasswordOut.setValue(r,parent.environmentSubstitute(inputGitPassword));

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
