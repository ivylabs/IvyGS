        <class_source><![CDATA[
import org.pentaho.di.core.plugins.PluginRegistry;
import org.pentaho.di.core.plugins.PluginTypeInterface;
import java.util.List;
import java.lang.System;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	boolean exist = false;
	try {

		final List categories = PluginRegistry.getInstance().getCategories(Class.forName("org.pentaho.di.core.plugins.StepPluginType"));
    	for(int i=0;i<categories.size();i++){
			if("Repository".equalsIgnoreCase(categories.get(i).toString().trim())){
				exist = true;
				break;
			}
    	}
   	} catch(ClassNotFoundException e) {
		// Nothing
   	}

	get(Fields.Out, "classExist").setValue(r, exist);
    r = createOutputRow(r, data.outputRowMeta.size());

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
